{
	"schemaVersion": 1,
	"id": "cobblebreeding",
	"version": "${version}",
	"name": "CobbleBreeding",
	"description": "Cobblebreeding is a Minecraft Fabric mod that offers customizable breeding for cobblemon",
	"authors": [
		"Hysocs"
	],
	"contact": {
		"discord": "https://discord.gg/KQyPEye7CT"
	},
	"license": "GNU Lesser General Public License v3",
	"icon": "assets/cobblebreeding/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"value": "com.cobblebreeding.CobblemonBreeding",
				"adapter": "kotlin"
			}
		],
		"client": [
			{
				"value": "com.cobblebreeding.CobbleBreedingClient",
				"adapter": "kotlin"
			}
		],
		"fabric-datagen": [
			{
				"value": "com.cobblebreeding.CobbleBreedingDataGenerator",
				"adapter": "kotlin"
			}
		]
	},
	"mixins": [
		"cobblebreeding.mixins.json",
		{
			"config": "cobblebreeding.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.14.13",
		"minecraft": "~1.21.1",
		"java": ">=21",
		"fabric-api": "*",
		"fabric-language-kotlin": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}